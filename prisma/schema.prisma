datasource postgresql {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

generator client {
  provider      = "prisma-client-js"
  previewFeatures = ["nApi"]
  binaryTargets = ["native"]
}

model Link {
  id        String   @id @default(cuid())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  name      String   @unique
  url       String
}
